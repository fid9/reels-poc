{
  "name": "nestjs-template-sample-app",
  "version": "0.0.0",
  "description": "",
  "author": "Povio Labs <info@poviolabs.com>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && yarn run task:version",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:tsnode": "node -r ts-node/register -r tsconfig-paths/register src/main.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./jest.config.js",
    "test:watch": "jest --watch --config ./jest.config.js",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config ./jest.config.js --runInBand",
    "test:e2e": "jest --config ./jest.config.js",
    "db:start": "docker-compose --env-file .env.local up",
    "task:version": "node ./utils/version.js ./dist/version.js",
    "task:clean": "rimraf dist",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ormconfig.js",
    "migration:show": "yarn typeorm:cli migration:show",
    "migration:run": "yarn typeorm:cli migration:run",
    "migration:generate": "yarn typeorm:cli migration:generate",
    "migration:revert": "yarn typeorm:cli migration:revert",
    "ci": "yarn install --immutable",
    "generate": "NODE_OPTIONS='-r ts-node/register' nest g -c ./guidelines/schematics"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/typeorm": "8.0.2",
    "@sentry/hub": "^6.2.3",
    "@sentry/integrations": "^6.2.3",
    "@sentry/minimal": "^6.2.3",
    "@sentry/node": "^6.2.3",
    "@sentry/types": "^6.2.3",
    "@types/lodash": "^4.14.168",
    "@types/luxon": "^1.27.1",
    "@types/request-ip": "^0.0.37",
    "@types/uuid": "^8.3.1",
    "aws-sdk": "^2.834.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^10.0.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "express-basic-auth": "^1.2.1",
    "isomorphic-git": "^1.9.2",
    "jest-junit": "^12.2.0",
    "lodash": "^4.17.21",
    "luxon": "^2.0.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "short-uuid": "^4.1.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.31",
    "typescript": "4.2.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "engine": {
    "yarn": "berry"
  }
}
